type Navigation {
  id: ID! @id
  name: String!
  type: NavigationTypes
  items: [NavigationItem] @relation(link: TABLE, onDelete: SET_NULL)
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}

enum NavigationTypes {
  TOP
  BOTTOM
}

type NavigationItem {
  id: ID! @id
  title: String!
  url: String
  items: [NavigationItem] @relation(link: TABLE, onDelete: SET_NULL)
}
